{
    "name": "gansel-rechtsanwaelte/lf-api",
    "type": "library",
    "description": "API for LF",
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Stark",
            "email": "oskarstark@googlemail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ramsey/uuid": "^3.9",
        "symfony/http-client": "^4.4",
        "symfony/mime": "^4.4",
        "symfony/string": "^5.0",
        "thecodingmachine/safe": "^1.0",
        "webmozart/assert": "^1.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Gansel\\LF\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gansel\\LF\\Api\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    }
}
